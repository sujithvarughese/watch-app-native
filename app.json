{
  "expo": {
    "name": "Authentime",
    "slug": "watch-app-native",
    "version": "1.0.15",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "watchappnative",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sujithv.watchappnative",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "backgroundColor": "#08253A"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#08253A"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sujithv.watchappnative"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#08253A"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Authentime to access your camera"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "68bd456f-da58-4f07-b189-48a2f0d5fad6"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/68bd456f-da58-4f07-b189-48a2f0d5fad6"
    }
  }
}
